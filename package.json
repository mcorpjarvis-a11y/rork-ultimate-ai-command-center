{
  "name": "rork-ultimate-ai-command-center",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "node scripts/ensure-deps.js",
    "prestart": "node scripts/prevent-ios.js && node scripts/ensure-deps.js",
    "start": "expo start --config metro.config.proxy.js",
    "start:all": "node scripts/start-all.js",
    "start:backend": "NODE_OPTIONS='--disable-proto=throw' ts-node --transpile-only backend/server.express.ts",
    "start:backend:prod": "node backend/dist/server.express.js",
    "dev:backend": "tsx watch backend/server.express.ts",
    "build:backend": "tsc -p backend/tsconfig.json",
    "start:frontend": "expo start --config metro.config.proxy.js",
    "start-web": "expo start --web --config metro.config.proxy.js",
    "start-web-dev": "DEBUG=expo* expo start --web --config metro.config.proxy.js",
    "check:node": "node scripts/check-node-version.js",
    "check:native-mods": "node scripts/check-native-mods.js",
    "reset:cache": "./scripts/reset-cache.sh",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:auth": "jest services/auth",
    "test:all": "node scripts/test-pipeline.js",
    "test:startup": "node scripts/test-startup.js",
    "test:expo-go": "node scripts/test-expo-go.js",
    "test:api-keys": "node scripts/test-api-keys.js",
    "test:metro-config": "node scripts/test-metro-config.js",
    "test:provider-registry": "node scripts/test-provider-registry.js",
    "test:login-pipeline": "tsx tests/testLoginPipeline.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "verify": "npm run verify:metro && npm test && npm run lint",
    "verify:backend": "npm run build:backend && node scripts/scan-backend-imports.js && node scripts/verify-backend-isolated.js",
    "verify:startup": "node scripts/test-startup.js",
    "verify:startup-order": "node scripts/verify-startup-order.js",
    "verify:api-config": "node scripts/verify-api-config.js",
    "verify:metro": "node scripts/verify-metro.js",
    "verify:all": "node scripts/verify-startup-order.js && npm run verify:metro && npm test && npm run verify:backend",
    "scan:repo": "npm run lint && npm run test:all && npm run verify:metro && npm run verify:backend && npm run verify:api-config",
    "lint": "expo lint",
    "android": "expo run:android",
    "build:apk": "expo run:android --variant release",
    "stress:e2e": "node scripts/stress-e2e.js"
  },
  "engines": {
    "node": ">=20.0.0 <23.0.0"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.62",
    "@ai-sdk/react": "^2.0.78",
    "@expo/vector-icons": "^15.0.3",
    "@hono/node-server": "^1.12.0",
    "@hono/trpc-server": "^0.4.0",
    "@jarvis/toolkit": "file:jarvis-toolkit",
    "@nkzw/create-context-hook": "^1.1.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native/virtualized-lists": "^0.82.1",
    "@react-navigation/native": "^7.1.6",
    "@stardazed/streams-text-encoding": "^1.0.2",
    "@tanstack/eslint-plugin-query": "^5.91.2",
    "@tanstack/react-query": "^5.90.5",
    "@trpc/client": "^11.6.0",
    "@trpc/react-query": "^11.6.0",
    "@trpc/server": "^11.6.0",
    "@ungap/structured-clone": "^1.3.0",
    "ai": "^5.0.76",
    "compression": "^1.7.5",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "expo": "54.0.23",
    "expo-asset": "~12.0.9",
    "expo-audio": "~1.0.14",
    "expo-auth-session": "~7.0.8",
    "expo-blur": "~15.0.7",
    "expo-clipboard": "~8.0.0",
    "expo-constants": "~18.0.10",
    "expo-crypto": "~15.0.7",
    "expo-document-picker": "~14.0.7",
    "expo-file-system": "~19.0.17",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-location": "~19.0.7",
    "expo-media-library": "~18.2.0",
    "expo-notifications": "~0.32.12",
    "expo-router": "~6.0.14",
    "expo-secure-store": "~15.0.7",
    "expo-speech": "~14.0.7",
    "expo-speech-recognition": "^3.0.0",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-web-browser": "~15.0.9",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "hono": "^4.10.4",
    "lucide-react-native": "^0.553.0",
    "nativewind": "^4.1.23",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.76.3",
    "react-native-device-info": "11.1.0",
    "react-native-gesture-handler": "2.28.0",
    "react-native-get-random-values": "1.11.0",
    "react-native-linear-gradient": "2.6.2",
    "react-native-mmkv": "3.1.0",
    "react-native-modal": "13.0.1",
    "react-native-reanimated": "4.1.5",
    "react-native-root-siblings": "5.0.0",
    "react-native-root-toast": "4.0.0",
    "react-native-safe-area-context": "5.6.2",
    "react-native-screens": "4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-url-polyfill": "2.0.0",
    "react-native-vector-icons": "10.0.0",
    "react-native-web": "0.21.2",
    "react-native-worklets": "0.6.1",
    "superjson": "^2.2.3",
    "tsx": "^4.19.1",
    "ws": "^8.18.0",
    "zod": "^4.1.12",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@expo/ngrok": "^4.1.0",
    "@playwright/test": "1.56.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jest": "29.5.14",
    "@types/node": "^24.10.0",
    "@types/react": "~19.1.10",
    "@types/ws": "^8.5.13",
    "babel-plugin-module-resolver": "^5.0.2",
    "esbuild": "^0.27.0",
    "eslint": "9.39.1",
    "eslint-config-expo": "~10.0.0",
    "jest": "~29.7.0",
    "jest-expo": "^54.0.13",
    "prettier": "3.6.2",
    "react-test-renderer": "19.0.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.2"
  },
  "private": true,
  "overrides": {
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native-renderer": "19.0.0",
    "react-test-renderer": "19.0.0"
  },
  "resolutions": {
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native-renderer": "19.0.0",
    "react-test-renderer": "19.0.0"
  },
  "expo": {
    "install": {
      "exclude": [
        "react",
        "react-dom",
        "react-native",
        "react-native-renderer",
        "react-test-renderer"
      ]
    }
  }
}
